{
  "name": "hotel_booking_app",
  "version": "1.0.0",
  "description": "Hotel booking application with Fastify and Turso DB",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc ",
    "start": "node dist/server.js",
    "dev": "tsnd --respawn --transpile-only --exit-child src/server.ts",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --fix --ext .ts src/",
    "test": "jest",
    "migrate": "tsx migrations/run.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:pull": "drizzle-kit pull"
  },
  "keywords": [
    "hotel",
    "booking",
    "fastify",
    "turso",
    "drizzle",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.817.0",
    "@aws-sdk/lib-storage": "^3.817.0",
    "@fastify/cors": "^9.0.0",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^7.7.3",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@firebase/auth": "^1.10.4",
    "@types/nodemailer": "^6.4.17",
    "dayjs": "^1.11.13",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.38.2",
    "expo-server-sdk": "^3.15.0",
    "fastify": "^4.22.2",
    "fastify-plugin": "^4.5.1",
    "fastify-zod": "^1.4.0",
    "firebase": "^11.7.3",
    "firebase-admin": "^13.0.2",
    "nodemailer": "^7.0.5",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "razorpay": "^2.9.2",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.1",
    "zod": "^3.24.4",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@libsql/client": "^0.14.0",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.8",
    "drizzle-kit": "^0.30.1",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
